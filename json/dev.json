{
    "nginx":{
        "server_token":"off",
        "default_site_enabled":false
    },
    "camper":{
        "site":{
            "dir":"\/var\/www\/camper",
            "name":"camper"
        },
        "static":[
            "jpg",
            "jpeg",
            "png",
            "gif",
            "ico",
            "css",
            "js",
            "svg",
            "woff"
        ],
        "expire":"10d",
        "s3-bucket":"camper-website"
    },
    "awscli":{
        "config_profiles":{
            "default":{
                "region":"ap-southeast-2",
                "aws_access_key_id":"",
                "aws_secret_access_key":""
            }
        }
    }
}